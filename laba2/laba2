#include <stdio.h>
#include <math.h>

int main()
{
    double a, x, G, F, Y, z, l;
    int c;
    printf("Enter c = 1 or 2 or 3:"); //Выбор функции для дальнейшего расчета
    scanf("%d", &c);
    printf ("Enter a =  ");
    scanf ("%lf", &a);
    printf ("Enter x = ");
    scanf ("%lf", &x);

    switch (c) { //Выполнение вычислений исходя из выбранной формулы
        case 1:
            z = (27 * pow(x, 2) + 33 * a * x + 10 * pow(x, 2));
            if (fabs(z) > 10e-5) {
                G = -(8 * (7 * pow(a, 2) + 34 * a * x - 5 * pow(x, 2))) / (27 * pow(x, 2) + 33 * a * x + 10 * pow(x, 2));
                printf("G=%f\n\n", G);
            }
            else printf("Не входит в область определения\n");
            break;

        case 2:
            F = -(1) / (sin(72 * pow(a, 2) - 5 * a * x - 12 * pow(x, 2) - M_1_PI / 2));
            printf("F=%f\n\n", F);
            break;

        case 3:
            l = (42 * pow(a, 2) + 53 * a * x + 15 * pow(x, 2) + 1);
            if (l > 0) {
                Y = log(42 * pow(a, 2) + 53 * a * x + 15 * pow(x, 2) + 1);
                printf("Y=%f\n\n", Y);
            }else printf("Не входит в область определения\n");
            break;

        default:
            printf("error");
    }
    return 0;
}
